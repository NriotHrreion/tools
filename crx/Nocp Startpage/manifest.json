{
    "name": "Nocp Startpage",
    "author": "NriotHrreion",
    "description": "By NriotHrreion",
    "version": "1.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["js/background.js"]
    },
    "chrome_url_overrides": {
        "newtab": "nstart.html" 
    },
    "options_page": "options.html",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "contextMenus"
    ]
}
